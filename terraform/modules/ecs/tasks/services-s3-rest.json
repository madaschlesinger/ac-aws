[
  {
    "name": "services-s3-rest",
    "image": "johnisadaptive/services-s3-rest",
    "cpu": 200,
    "memory": 300,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8088
      }
    ],
    "environment": [
        {
          "name": "S3_BUCKET",
          "value": "adaptive-bucket"
        }
      ]

  }
]
