{
  "id": "marathon-lb",
  "cpus": 0,
  "mem": 0,
  "cmd": "/marathon-lb/run sse --marathon http://10.1.7.40:8080 --group external",
  "env": {
    "PORTS": "80,81,443,9090"
  },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "mesosphere/marathon-lb",
      "privileged": true,
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 80, "hostPort": 0, "servicePort": 19000 },
        { "containerPort": 81, "hostPort": 0, "servicePort": 19001 },
        { "containerPort": 443, "hostPort": 0, "servicePort": 19002 },
        { "containerPort": 9090, "hostPort": 0, "servicePort": 19003 }
      ]
    }
  }
}
