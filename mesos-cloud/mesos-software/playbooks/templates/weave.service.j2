{% for host in groups[master] %}

# {{ansible_managed}}

[Unit]
Description=Zookeeper
After=docker.service
Wants=docker.service

[Service]
ExecStart=/usr/local/bin/weave launch {{ hostvars[host]['ec2_private_ip_address']}} 
Restart=always
RestartSec=20

[Install]
WantedBy=multi-user.target

{% endfor %}
