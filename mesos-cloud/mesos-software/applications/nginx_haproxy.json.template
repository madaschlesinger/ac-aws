{
  "id": "nginx",
  "cpus": 0.3,
  "mem": 100.0,
  "labels": {
        "HAPROXY_GROUP" : "external",
        "HAPROXY_0_VHOST" : "##EXTERNAL_EC2_IP##"
      },
  "hostname" : "nginx.weave.local",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "nginx",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 80, "hostPort": 0, "servicePort": 3000 }
      ]
    }
  }
}
